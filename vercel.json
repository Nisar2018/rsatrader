{
  "rewrites": [
    
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    
    {
      "source": "/(.*)",
      "destination": "/frontend/build/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|ico|gif|jpg|png|svg|eot|ttf|woff|woff2)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}